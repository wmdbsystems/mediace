{
	"name": "friendsoftypo3/mediace",
	"type": "typo3-cms-extension",
	"description": "TYPO3 Extension to add Media-related Content Elements to your website.",
	"homepage": "https://github.com/FriendsOfTYPO3/mediace",
	"license": ["GPL-2.0+"],

	"require": {
		"typo3/cms-core": "~7.5",
		"typo3/cms-frontend": "~7.5"
	},
	"replace": {
		"mediace": "*"
	},
	"extra": {
		"typo3/class-alias-loader": {
			"class-alias-maps": [
				"Migrations/Code/ClassAliasMap.php"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"FoT3\\Mediace\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FoT3\\Mediace\\Tests\\": "Tests/"
		}
	}
}